  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KamataRyo/board_server/issues"
  },
  "homepage": "https://github.com/KamataRyo/board_server#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "forever": "^0.15.3",
    "immutability-helper": "^2.1.2",
    "jsonwebtoken": "^7.3.0",
    "material-ui": "^0.17.4",
    "mongoose": "^4.9.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",

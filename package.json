{
  "name": "board_server",
  "version": "1.0.0",
  "description": "This is a chat board server.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel ./server --out-dir ./lib --source-maps inline",
    "start": "webpack-dev-server --progress --colors --hot --inline --watch & PS=$! & npm run build && node ./lib/index.js && kill $PS",
    "lint": "eslint . --ext=js --ext=jsx",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamataRyo/board_server.git"
  },
  "author": "kamataryo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KamataRyo/board_server/issues"
  },
  "homepage": "https://github.com/KamataRyo/board_server#readme",
  "dependencies": {
    "express": "^4.15.2",
    "immutability-helper": "^2.1.2",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
